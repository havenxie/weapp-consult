/* pages/feedback/feedback.wxss *//* pages/feedback/feedback.wxss */
.form {
  width: 100%;
  height: 100%;
  font-size: 32rpx;  
}
.textArea {
  width: 100%;
  height: 30%;
  margin-top: 20rpx;
  padding: 30rpx 30rpx;
  line-height: 40rpx;
  background-color: #fff;
  box-sizing: border-box;
  overflow-x: hidden;
  overflow-y: scroll;
  color: #333;
}
.submit {
  width: 90%;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50rpx;
  border-width: 0rpx;
  background-color: #088;
  color: #fff;
  margin-top: 100rpx;
  font-size: 36rpx;
}