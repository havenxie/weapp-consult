.pages{
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: space-around; */
  justify-content: flex-start;
}
/* 主页界面开始 */
.maintxt {
  font-size: 36rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-weight: bold;
}

.showbox{
  width: 100%;
  padding: 0rpx;
  /* box-shadow: 8rpx 8rpx 10rpx rgb(219, 219, 219); */
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  box-sizing: border-box;
  margin-top: 4%;
}

.enter {
  color: #fff;
  background-color: #5fbcf1;
  padding: 0rpx;
}
.enter:active{
  color: #333;
  background-color: #ccc;
  opacity: 0.5;
}
.enter .entertxt{
  font-size: 40rpx;
  font-weight: bold;
}
.enter .line{
  width: 70%;
  height: 0;
  border-top: 2rpx solid #fff;
  box-shadow: 3rpx 3rpx 5rpx rgb(230, 230, 230);
}
.enter .tipstxt{
  font-size: 30rpx;
  color: #fff;
  opacity: 0.8;
}

